{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/HomePage/AllData.js", "src/app/HomePage/page.jsx", "src/Provider/NextAuthProvider.tsx", "src/app/dashboard/AllUsers/page.tsx", "src/app/api/getUsers/route.ts", "src/components/AllUserData/page.tsx", "src/app/dashboard/userProfile/page.jsx", "src/app/component/AllData.js"],
  "exclude": ["node_modules"]
}
